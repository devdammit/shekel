enum AccountType {
    CASH
    CREDIT
    DEBIT
}

type Account {
    id: Uint64!
    name: String!
    description: String
    Type: AccountType!
    Balance: Amount!

    deletedAt: DateTime
    createdAt: DateTime!
    updatedAt: DateTime!
}


input CreateAccountInput {
    name: String!
    description: String
    Type: AccountType!
    Balance: AmountInput!
}